  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.67.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.24.8",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "reactflow": "^11.11.4",
    "typescript": "^5.8.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--no-deprecation react-scripts start",
    "build": "cross-env NODE_OPTIONS=--no-deprecation react-scripts build",
    "build:staging": "cross-env REACT_APP_ENV=staging npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\" \"*.{js,json,md}\"",
    "check-format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\" \"*.{js,json,md}\"",
    "dev": "react-scripts start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^src/lib/supabase$": "<rootDir>/src/lib/__mocks__/supabase.js"
    }
  },
  "proxy": "http://localhost:3000",
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^10.1.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "supabase": "^2.20.5",
    "tailwindcss": "^3.4.17",
    "tempo-devtools": "^2.0.100"
  }
}
